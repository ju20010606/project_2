<head>
    <link rel="stylesheet" href="/css/alerts.css">
</head>
 <% if (alerts.error) { %>
    <% alerts.error.forEach(msg => { %>
        <div class="alertsContainerTwo"></div>
        <h4><%=  msg %></h4>
        </div>
    <% }) %>
<% } %>
<% if (alerts.success) { %>
    <% alerts.success.forEach(msg => { %>
        <div class="alertsContainer">
       <h4><%= msg %></h4>
       </div>
    <% }) %>
<% } %>

<script src="js/alerts.js"></script>